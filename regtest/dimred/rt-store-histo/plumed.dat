x: DISTANCE ATOMS=1,2
y: COLLECT_FRAMES ARG=x STRIDE=1
c: EUCLIDEAN_DISSIMILARITIES USE_OUTPUT_DATA_FROM=y 
m: CLASSICAL_MDS USE_OUTPUT_DATA_FROM=c NLOW_DIM=1

HISTOGRAM ...
  ARG=m.*
  GRID_MIN=-1.0
  GRID_MAX=1.0
  GRID_BIN=100
  BANDWIDTH=0.1
  LABEL=hC
... HISTOGRAM

PRINT ARG=x FILE=colv
DUMPGRID GRID=hC FILE=histoC FMT=%8.4f
